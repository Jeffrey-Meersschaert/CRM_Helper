{
  "manifest_version": 3,
  "name": "FT CRME Helper",
  "description": "Base Level Extension",
  "version": "2.0",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["contextMenus","clipboardRead"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["Options/ButtonOptions.txt"],
      "matches": [ "https://*/*" ]
    }
  ],
  "icons": {
    "16": "images/CRME.png"
  }
}
